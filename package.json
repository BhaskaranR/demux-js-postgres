  },
  "peerDependencies": {
    "demux": "4.0.0"
  },
  "scripts": {
    "build": "tsc && cp -R src/cyanaudit dist",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "PGPASSWORD=docker jest",
    "build-docs": "./scripts/build-docs.sh",
    "current-version": "echo $npm_package_version"
