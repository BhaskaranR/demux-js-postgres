  },
  "license": "MIT",
  "devDependencies": {
    "@blockone/tslint-config-blockone": "^2.0.0",
    "@types/bunyan": "^1.8.5",
    "@types/dockerode": "^2.5.5",
    "@types/jest": "^23.1.4",
    "@types/massive": "^4.6.1",
    "@types/node": "^10.5.1",
    "@types/request-promise-native": "^1.0.15",
    "dockerode": "^2.5.5",
    "jest": "^22.4.3",
    "pg-monitor": "^1.1.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
